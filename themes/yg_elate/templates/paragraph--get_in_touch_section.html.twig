<section id="fh5co-contact" data-section="contact">
		<div class="container">
			<div class="row">
				<div class="col-md-12 section-heading text-center">
					<h2 class="to-animate">{{content.field_title}}</h2>
					<div class="row">
						<div class="col-md-8 col-md-offset-2 subtext to-animate">
							<h3>{{content.field_description}}</h3>
						</div>
					</div>
				</div>
			</div>
			<div class="row row-bottom-padded-md">
				<div class="col-md-6 to-animate">
					<h3>Contact Info</h3>
					<ul class="fh5co-contact-info">
						<li class="fh5co-contact-address ">
							<i class="icon-home"></i>
							{{content.field_address}}
						</li>
						<!-- <li><i class="icon-phone"></i> (123) 465-6789</li> -->
						<li><i class="icon-envelope"></i>{{content.field_email}}</li>
						<li><i class="icon-globe"></i> <a href="{{content.field_website.0}}" target="_blank">{{content.field_website}}</a></li>
					</ul>
				</div>

				<div class="col-md-6 to-animate">
					<h3>Contact Form</h3>
					{{content.field_contact}}
				</div>

			</div>
		</div>
		
	</section>