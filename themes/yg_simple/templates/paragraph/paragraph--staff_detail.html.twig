<!-- Staff Details Item-->
<div class="col-md-4 fh5co-staff">
<img src="{{ file_url(content.field_image['#items'].entity.uri.value) }}" alt="{{ content.field_title.0 }}" class="img-responsive">
	<h3>{{ content.field_title.0 }}</h3>
	<h4>{{ content.field_service_list.0 }}</h4>
	{{ content.field_description.0 }}
	<ul class="fh5co-social">
		{% if content.field_google_plus.0|render is not empty %}
			<li><a href="{{ content.field_google_plus[0]['#url']|render }}"><i class="icon-google"></i></a></li>
		{% endif %}	
		{% if content.field_dribbble.0|render is not empty %}
			<li><a href="{{ content.field_dribbble[0]['#url']|render }}"><i class="icon-dribbble"></i></a></li>
		{% endif %}	
		{% if content.field_twitter.0|render is not empty %}
			<li><a href="{{ content.field_twitter[0]['#url']|render }}"><i class="icon-twitter"></i></a></li>
		{% endif %}	
		{% if content.field_facebook.0|render is not empty %}
			<li><a href="{{ content.field_facebook[0]['#url']|render }}"><i class="icon-facebook"></i></a></li>
		{% endif %}	
		{% if content.field_instagram.0|render is not empty %}
			<li><a href="{{ content.field_instagram[0]['#url']|render }}"><i class="icon-instagram"></i></a></li>
		{% endif %}	
	</ul>
</div>